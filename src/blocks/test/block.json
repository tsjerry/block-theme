{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "block-theme/test",
	"version": "0.1.0",
	"title": "Test blok",
	"category": "media",
	"icon": "smiley",
	"description": "Blokje om te testen",
	"supports": {
		"align": true,
		"html": false,
		"dimensions": {
			"aspectRatio": true
		}
	},
	"textdomain": "test",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"attributes": {
		"paragraph": {
			"type": "string",
			"source": "rich-text",
			"selector": "p",
			"default": "Jo!"
		}
	}
}